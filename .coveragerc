[run]
source = src
omit =
    */tests/*
    */venv/*
    */__pycache__/*
    */f1-strat-ai-qa-framework/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    pass

[html]
directory = htmlcov
